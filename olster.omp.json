{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\uf303"
          },
          "leading_diamond": "\u256d\u2500\ue0b6",
          "template": " {{.Icon}} ",
          "foreground": "#A2B0B0",
          "background": "#546E6E",
          "type": "os",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "{{ .Path }}",
          "foreground": "#364545",
          "background": "#121414",
          "type": "path",
          "template": "<parentBackground>\ue0b4</> {{.Path}} \uf0e7",
          "style": "powerline"
        },
        {
          "properties": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "template": " {{ .HEAD }} {{ if .Working.Changed }}{{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}<#9C3340> \uf046 {{ .Staging.String }}</>{{ end }} ",
          "foreground": "#9c9c9c",
          "powerline_symbol": "\ue0b0",
          "background": "#192929",
          "type": "git",
          "style": "powerline",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#213636{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#4A3315{{ end }}",
            "{{ if gt .Ahead 0 }}#9EADAD{{ end }}",
            "{{ if gt .Behind 0 }}#9EADAD{{ end }}"
          ],
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#9EADAD{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#213636{{ end }}",
            "{{ if gt .Ahead 0 }}#3C6653{{ end }}",
            "{{ if gt .Behind 0 }}#706223{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\uf303"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ if .Env.USER }}{{ .Env.USER }}{{ else }}{{ .Env.USERNAME }}{{ end }}",
          "foreground": "#4A5C5C",
          "background": "#2A3232",
          "type": "text",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "fetch_package_manager": true,
            "npm_icon": " <#45475A>\ufbfe</>",
            "yarn_icon": " <#348cba>\uf61a</>"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "\ue718 {{ .Full }}",
          "foreground": "#334F44",
          "background": "#91A8A8",
          "type": "node",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": " \ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": "{{ .CurrentDate | date .Format }} \uf0e7 ",
          "foreground": "#39484D",
          "background": "#DAE4F2",
          "type": "time",
          "style": "diamond",
          "invert_powerline": true
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "\u2570\u2500",
          "foreground": "#21c7c7",
          "type": "text",
          "style": "plain"
        },
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ{{ if gt .Code 0 }}\uf00d{{ else }}{{ end }} ",
          "foreground": "#e0f8ff",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ef5350{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
